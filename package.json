{
  "name": "graphevolution",
  "version": "1.0.0",
  "productName": "GraphEvolution",
  "description": "Interactive evolutinary algorithm for evolving grammar for graph visualisation Vega.js",
  "main": "main.js",
  "scripts": {
   "start": "electron .",
   "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/img/icon.icsn --prune=true --out=release-builds",
   "package-win": "electron-packager . graphevolution --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/img/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"GraphEvolution\"",
   "package-linux": "electron-packager . graphevolution --overwrite --asar=true --platform=linux --arch=x64 --icon=app/img/512x512.png --prune=true --out=release-builds"
  },
  "author": "Petr Hanzl",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.5",
    "electron": "^1.8.8",
    "electron-connect": "^0.6.3",
    "gulp": "^4.0.1",
    "gulp-sass": "^4.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "css-toggle-switch": "^4.1.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "vega-embed": "^3.7.2",
    "vega-lite": "^2.4.1"
  }
}
